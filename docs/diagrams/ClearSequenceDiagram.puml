@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":McGymmyParser" as McGymmyParser LOGIC_COLOR
participant "executable:ClearCommand" as ClearCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("clear")
activate LogicManager

LogicManager -> McGymmyParser : parseCommand("clear")
activate McGymmyParser

McGymmyParser --> LogicManager : executable
deactivate McGymmyParser

LogicManager -> ClearCommand : execute()
activate ClearCommand

ClearCommand -> Model : clearFilteredFood()
activate Model

Model --> ClearCommand
deactivate Model

create CommandResult
ClearCommand -> CommandResult
activate CommandResult

CommandResult --> ClearCommand
deactivate CommandResult

ClearCommand --> LogicManager : result
deactivate ClearCommand

[<--LogicManager
deactivate LogicManager
@enduml
